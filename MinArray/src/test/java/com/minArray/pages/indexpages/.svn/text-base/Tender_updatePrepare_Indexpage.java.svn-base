	@FindBy(xpath = "//form/table/tbody/tr/td/table/tbody/tr[1]/td/table/tbody/tr/td/table/tbody/tr[4]/td/input")
	private WebElement upload_file_browse;

	@FindBy(xpath = "//table/tbody/tr[2]/td[2]/div/table/tbody/tr[2]/td/button")
	private WebElement upload_file_ok;

	@FindBy(xpath = "//div/table/tbody/tr[2]/td/table/tbody/tr/td/button")
	private WebElement file_upload_close;

	public Tender_creation_DashboardPage upload_document(
			WebElement upload_button) {
		common.pause(3);
		upload_button.click();
		common.pause(2);
		upload_file_browse.click();
		common.pause(2);

		try {

			Robot r = new Robot();

			r.keyPress(KeyEvent.VK_D); // d
			r.keyRelease(KeyEvent.VK_D);
			common.pause(1);
			r.keyPress(KeyEvent.VK_PERIOD); // d
			r.keyRelease(KeyEvent.VK_PERIOD);
			common.pause(1);
			r.keyPress(KeyEvent.VK_T); // t
			r.keyRelease(KeyEvent.VK_T);
			common.pause(1);
			r.keyPress(KeyEvent.VK_X); // x
			r.keyRelease(KeyEvent.VK_X);
			common.pause(1);
			r.keyPress(KeyEvent.VK_T); // t
			r.keyRelease(KeyEvent.VK_T);
			common.pause(1);
			r.keyPress(KeyEvent.VK_TAB); // t
			r.keyRelease(KeyEvent.VK_TAB);
			common.pause(1);
			r.keyPress(KeyEvent.VK_TAB); // t
			r.keyRelease(KeyEvent.VK_TAB);
			common.pause(2);
			r.keyPress(KeyEvent.VK_ENTER); // confirm by pressing Enter in the
			r.keyRelease(KeyEvent.VK_ENTER); // end

		} catch (AWTException e) {

			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		common.pause(5);
		upload_file_ok.click();

		common.pause(2);

		file_upload_close.click();

		common.pause(2);

		return new Tender_creation_DashboardPage(driver);
	}

