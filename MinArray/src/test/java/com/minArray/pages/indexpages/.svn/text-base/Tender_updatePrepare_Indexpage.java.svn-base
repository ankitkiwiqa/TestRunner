	/*----------------------------------------------Method to upload document-----------------*/

	public Tender_creation_DashboardPage scope_of_work_supplies_document_upload(
			WebElement element) {

		Actions move_to_element = new Actions(driver);
		move_to_element.moveToElement(element).build().perform();

		common.pause(4);
		element.click();
		common.pause(2);
		try {

			Robot r = new Robot();

			common.pause(3);
			r.keyPress(KeyEvent.VK_D); // d
			r.keyRelease(KeyEvent.VK_D);
			common.pause(1);
			r.keyPress(KeyEvent.VK_PERIOD); // d
			r.keyRelease(KeyEvent.VK_PERIOD);
			common.pause(1);
			r.keyPress(KeyEvent.VK_T); // t
			r.keyRelease(KeyEvent.VK_T);
			common.pause(1);
			r.keyPress(KeyEvent.VK_X); // x
			r.keyRelease(KeyEvent.VK_X);
			common.pause(1);
			r.keyPress(KeyEvent.VK_T); // t
			r.keyRelease(KeyEvent.VK_T);

			common.pause(3);
			r.keyPress(KeyEvent.VK_TAB); // t
			r.keyRelease(KeyEvent.VK_TAB);
			common.pause(3);
			r.keyPress(KeyEvent.VK_TAB); // t
			r.keyRelease(KeyEvent.VK_TAB);
			common.pause(3

			);

			r.keyPress(KeyEvent.VK_ENTER); // confirm by pressing Enter in the
											// end
			r.keyRelease(KeyEvent.VK_ENTER);

		} catch (AWTException e) {

			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		common.pause(5);
		upload_file_ok.click();
		common.pause(3);

		return new Tender_creation_DashboardPage(driver);
	}

